package com.sample;
import java.util.Scanner;
public class Elecbill {
     public static void main(String[] args)
     {
    	 Scanner scanner=new Scanner(System.in);
    	 int reading=scanner.nextInt();
    	
    	 if(reading<=50)
    		 System.out.println("charge amount is:"+reading*1);
    	 else
    		 if(50<reading && reading<=100)
    			System.out.println("charge amount is:"+reading*2); 
    		 else
    			 if(100<reading && reading<=200)
    				 System.out.println("charge amount is:"+reading*3);
    			 else
    				 if(200<reading && reading<=300)
    					 System.out.println("charge amount is:"+reading*4);
    				 else
    					 System.out.println("charge amount is:"+reading*5);
    	 scanner.close();
     }
}
