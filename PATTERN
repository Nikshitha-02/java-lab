package com.sample;
import java.util.Scanner;
public class Pattern {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
Scanner scanner=new Scanner(System.in);
int n=scanner.nextInt();
for(int i=1;i<=n;i++)
{
	
	for(int j=i;j>=1;j--)
	{
		System.out.print(j);
	}
	for(int j=2;j<=i;j++)
	{
		System.out.print(j);
	}
	System.out.println();
}
	}
}



